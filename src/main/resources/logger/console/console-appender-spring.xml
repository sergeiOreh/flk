<?xml version="1.0" encoding="UTF-8"?>

<!--
Конфигурация Console appender logback  предоставленная для импорта, равнозначно программной
 инициализации выполняемой  Boot
-->

<included>
	<appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<charset>UTF-8</charset>
			<pattern>${FILE_LOG_PATTERN}</pattern>
		</encoder>
	</appender>
</included>
